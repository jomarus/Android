<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.photocad.comunicacionentreactivities.MathActivity"
    android:id="@+id/fondo">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Respuesta"
        android:id="@+id/textView_Respuesta"
        android:layout_marginLeft="59dp"
        android:layout_marginTop="87dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Duplicar"
        android:id="@+id/button_Duplicar"
        android:layout_marginTop="60dp"
        android:layout_below="@+id/textView_Respuesta"
        android:layout_alignLeft="@+id/textView_Respuesta"
        android:layout_alignStart="@+id/textView_Respuesta" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ACTIVITY 2"
        android:id="@+id/textView2"
        android:layout_alignParentTop="true"
        android:layout_alignLeft="@+id/textView_Respuesta"
        android:layout_alignStart="@+id/textView_Respuesta"
        android:layout_marginTop="26dp" />

</RelativeLayout>
